{
  "version": 1,
  "projects": {
    "clients": {
      "root": "libs/clients",
      "sourceRoot": "libs/clients/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/clients/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/clients"],
          "options": {
            "jestConfig": "libs/clients/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "contracts": {
      "root": "libs/contracts",
      "sourceRoot": "libs/contracts/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/contracts/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/contracts"],
          "options": {
            "jestConfig": "libs/contracts/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "front": {
      "projectType": "application",
      "root": "apps/front",
      "sourceRoot": "apps/front/src",
      "prefix": "log",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/front",
            "index": "apps/front/src/index.html",
            "main": "apps/front/src/main.ts",
            "polyfills": "apps/front/src/polyfills.ts",
            "tsConfig": "apps/front/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/front/src/favicon.ico", "apps/front/src/assets"],
            "styles": ["apps/front/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/front/src/environments/environment.ts",
                  "with": "apps/front/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "front:build:production"
            },
            "development": {
              "browserTarget": "front:build:development",
              "proxyConfig": "apps/front/src/proxy.conf.json"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "front:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/front/src/**/*.ts",
              "apps/front/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/front"],
          "options": {
            "jestConfig": "apps/front/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "ref": {
      "root": "libs/ref",
      "sourceRoot": "libs/ref/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/ref/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ref"],
          "options": {
            "jestConfig": "libs/ref/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "referential": {
      "root": "apps/referential",
      "sourceRoot": "apps/referential/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/referential",
            "main": "apps/referential/src/main.ts",
            "tsConfig": "apps/referential/tsconfig.app.json",
            "assets": ["apps/referential/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/referential/src/environments/environment.ts",
                  "with": "apps/referential/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "referential:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/referential/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/referential"],
          "options": {
            "jestConfig": "apps/referential/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "supply": {
      "root": "libs/supply",
      "sourceRoot": "libs/supply/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/supply/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/supply"],
          "options": {
            "jestConfig": "libs/supply/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
